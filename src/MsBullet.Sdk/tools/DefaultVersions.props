<?xml version="1.0" encoding="utf-8"?>
<Project>
  
  <Import Project="$(VersionsPropsPath)" Condition="Exists('$(VersionsPropsPath)')" />
  
  <!--
    MsBullet SDK features.
  -->
  <PropertyGroup>
    <!-- Opt-out features -->
    <UsingToolXUnit Condition="'$(UsingToolXUnit)' == ''">true</UsingToolXUnit>
    <UsingToolGitVersion Condition="'$(UsingToolGitVersion)' == ''">true</UsingToolGitVersion>
  </PropertyGroup>
  
  <!-- Libs -->
  <PropertyGroup> 
    <MicrosoftCodeAnalysisFxCopAnalyzersVersion Condition="'$(MicrosoftCodeAnalysisFxCopAnalyzersVersion)' == ''">2.9.8</MicrosoftCodeAnalysisFxCopAnalyzersVersion>
    <MicrosoftVisualStudioThreadingAnalyzersVersion Condition="'$(MicrosoftVisualStudioThreadingAnalyzersVersion)' == ''">16.5.132</MicrosoftVisualStudioThreadingAnalyzersVersion>
    <StyleCopAnalyzersVersion Condition="'$(StyleCopAnalyzersVersion)' == ''">1.1.118</StyleCopAnalyzersVersion>
    <MicrosoftNETTestSdkVersion Condition="'$(MicrosoftNETTestSdkVersion)' == ''">16.5.0</MicrosoftNETTestSdkVersion>
    <XUnitVersion Condition="'$(XUnitVersion)' == ''">2.4.1</XUnitVersion>
    <XUnitAssertVersion Condition="'$(XUnitAssertVersion)' == ''">$(XUnitVersion)</XUnitAssertVersion>
    <XUnitRunnerVisualstudioVersion Condition="'$(XUnitRunnerVisualstudioVersion)' == ''">$(XUnitVersion)</XUnitRunnerVisualstudioVersion>
    <XUnitRunnerConsoleVersion Condition="'$(XUnitRunnerConsoleVersion)' == ''">$(XUnitVersion)</XUnitRunnerConsoleVersion>
    <XUnitAbstractionsVersion Condition="'$(XUnitAbstractionsVersion)' == ''">2.0.3</XUnitAbstractionsVersion>
    <GitVersionTaskVersion Condition="'$(GitVersionTaskVersion)' == ''">5.2.4</GitVersionTaskVersion>
  </PropertyGroup>
  
</Project>
