<?xml version="1.0" encoding="utf-8"?>
<Project>

  <PropertyGroup>
    <XUnitVersion Condition="'$(XUnitVersion)' == ''">2.*</XUnitVersion>
    <XUnitAssertVersion Condition="'$(XUnitAssertVersion)' == ''">$(XUnitVersion)</XUnitAssertVersion>
    <XUnitAbstractionsVersion Condition="'$(XUnitAbstractionsVersion)' == ''">$(XUnitVersion)</XUnitAbstractionsVersion>
    <XUnitRunnerVisualStudioVersion Condition="'$(XUnitRunnerVisualStudioVersion)' == ''">2.*</XUnitRunnerVisualStudioVersion>
    <XUnitRunnerConsoleVersion Condition="'$(XUnitRunnerConsoleVersion)' == ''">2.*</XUnitRunnerConsoleVersion>
  </PropertyGroup>

  <PropertyGroup>
    <XUnitDesktopSettingsFile>$(MSBuildThisFileDirectory)xunit.runner.json</XUnitDesktopSettingsFile>
  </PropertyGroup>
  
</Project>
